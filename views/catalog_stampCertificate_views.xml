<odoo>
    <data>
        <record id="reclutamiento_kuale_stampCertificate_view_tree" model="ir.ui.view">
            <field name="name">reclutamiento__kuale.digital.stamp.certificate.view.tree</field>
            <field name="model">reclutamiento__kuale.digital.stamp.certificate</field>
            <field name="arch" type="xml">
                <tree string="Stamp Certificate">
                    <field name="name"/>
                    <field name="imss_registration_id"/>
                    <field name="certificate_number" />
                    <field name="password" />
                    <field name="confirmation" />
                    <field name="validity" />
                    <field name="valid_from" />
                    <field name="valid_to" />
                    <field name="status" />
                </tree>
            </field>
        </record>

        <record id="reclutamiento_kuale_stampCertificate_view_form" model="ir.ui.view">
            <field name="name">reclutamiento__kuale.digital.stamp.certificate.view.form</field>
            <field name="model">reclutamiento__kuale.digital.stamp.certificate</field>
            <field name="arch" type="xml">
                <form string="Stamp Certificate">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="imss_registration_id"/>
                            <field name="certificate_number" />
                            <field name="password" />
                            <field name="confirmation" />
                            <field name="validity" />
                            <field name="valid_from" />
                            <field name="valid_to" />
                            <field name="status" />
                        </group>
                    </group>
                </form>
            </field>
        </record>

        <record id="reclutamiento__kuale_stampCertificate_action" model="ir.actions.act_window">
            <field name="name">Stamp Certificate</field>
            <field name="res_model">reclutamiento__kuale.digital.stamp.certificate</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new stamp certificate
                </p>
            </field>
        </record>
    </data>
</odoo>
